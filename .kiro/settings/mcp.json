{
  "mcpServers": {
    "aws-iac": {
      "command": "python",
      "args": ["-m", "uv", "tool", "run", "awslabs.aws-iac-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "DEBUG"
      },
      "disabled": false,
      "autoApprove": ["search_cdk_documentation", "search_cdk_samples_and_constructs", "cdk_best_practices", "validate_cloudformation_template", "check_cloudformation_template_compliance"]
    }
  }
}